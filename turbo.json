{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:storybook": {
      "dependsOn": ["^build"]
    },
    "dev:external": {
      "cache": false,
      "persistent": true
    }
  }
}
