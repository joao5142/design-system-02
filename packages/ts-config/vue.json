{
  "include": [
    "index.ts",
    "env.d.ts",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.js"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "src/components/**/*.stories.ts",
    "src/components/**/*.stories.tsx"
  ],
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "verbatimModuleSyntax": false,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true
  }
}
