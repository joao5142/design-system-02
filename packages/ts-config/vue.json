{
  "include": [
    "index.ts",
    "env.d.ts",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.js"
  ],
  "exclude": [
    "src/components/**/*.stories.ts",
    "src/components/**/*.stories.tsx"
  ],
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "verbatimModuleSyntax": false,
    "allowJs": true
  }
}
