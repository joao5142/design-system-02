{
  "name": "@can-i-helpu-ds/vue",
  "author": "Can I help U",
  "version": "1.3.0",
  "description": "A design system  library for the Can I Help U project",
  "keywords": [
    "design-system",
    "tokens",
    "style",
    "can-i-helpu",
    "colors",
    "typography",
    "vue"
  ],
  "license": "ISC",
  "type": "module",
  "main": "./dist/bundle.umd.cjs",
  "module": "./dist/bundle.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bundle.js",
      "require": "./dist/bundle.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style": {
      "default": "./dist/style.css"
    }
  },
  "scripts": {
    "dev:external": "vite build --watch",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "storybook:fix:gp": "touch ./storybook-static/.nojekyll",
    "build:storybook": "storybook build &&  npm run storybook:fix:gp"
  },
  "dependencies": {
    "@can-i-helpu-ds/eslint-config": "*",
    "@can-i-helpu-ds/prettier-config": "*",
    "@can-i-helpu-ds/tokens": "*",
    "@can-i-helpu-ds/ts-config": "*",
    "@phosphor-icons/vue": "^2.2.1",
    "@vueuse/components": "^11.2.0",
    "@vueuse/core": "^11.2.0",
    "vite-plugin-vuetify": "^2.0.4",
    "vue": "^3.5.13",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.7.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-docs": "^8.4.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.2.9",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "2.1.2",
    "@vitest/ui": "^2.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "sass": "^1.77.8",
    "sass-embedded": "^1.77.8",
    "sass-loader": "^16.0.1",
    "storybook": "^8.2.9",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.3.1",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-singlefile": "^2.0.2",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.0.21",
    "vuetify": "^3.6.6"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  }
}
