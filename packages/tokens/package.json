{
  "name": "@can-i-helpu-ds/tokens",
  "license": "MIT",
  "version": "1.1.0",
  "author": "Can I help U",
  "description": "A ts-config library for the Can I Help U project",
  "keywords": [
    "tokens",
    "style",
    "typography",
    "ts"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts",
    "dev:external": "tsup src/index.ts --format=esm,cjs --dts --watch",
    "lint": "npx eslint '**/*.{js,ts,tsx}' --fix"
  },
  "devDependencies": {
    "@can-i-helpu-ds/eslint-config": "*",
    "@can-i-helpu-ds/ts-config": "*",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  }
}
